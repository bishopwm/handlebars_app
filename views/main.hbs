<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
<h1>My Plivo Dashboard</h1>
<hr>
<a href='/'>Home</a><br>
<a href='view-incoming-messages'>View Received Messages</a><br>
<a href='view-sent-messages'>View Sent Messages</a><br>
<a href='send-message'>Send New Message</a><br><br>


<button>Refresh Recent Messages</button>

  <ul>
    {{#each messageData}}
    <h5>Inbound Message</h5>
      <li>From: {{this.from_number_json}}</li>
      <li>To: {{this.to_number_json}}</li>
      <li>Message: {{this.text_json}}</li>
      <li>ID: {{this._id}}</li>
    <hr>
    {{/each}}
  </ul>

    <ul>
    {{#each sentMessageData}}
    <h5>Outbound Message</h5>
      <li>From: Plivo</li>
      <li>To: {{this.toNumber}}</li>
      <li>Message: {{this.messageText}}</li>
    <hr>
    {{/each}}
  </ul>
